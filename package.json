{
  "name": "uni-pages",
  "type": "module",
  "version": "1.0.0-beta.3",
  "packageManager": "pnpm@10.18.0",
  "description": "在 UniApp 页面文件中通过 definePageMeta 一键定义页面参数",
  "author": "skiyee <319619193@qq.com>",
  "license": "MIT",
  "homepage": "https://github.com/skiyee/uni-pages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/skiyee/uni-pages.git"
  },
  "bugs": "https://github.com/skiyee/uni-pages/issues",
  "keywords": [
    "uniapp",
    "uni-app",
    "pages.json",
    "definePageMeta",
    "definePage"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": {
        "import": "./dist/index.d.mts",
        "require": "./dist/index.d.cts"
      },
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.18.0"
  },
  "scripts": {
    "stub": "tsdown --watch",
    "dev": "pnpm stub",
    "doc": "pnpm --filter=./docs dev",
    "dev:h5": "pnpm build && pnpm --filter=./example dev:h5",
    "dev:mp-weixin": "pnpm build && pnpm --filter=./example dev:mp-weixin",
    "build": "tsdown",
    "build:h5": "pnpm build && pnpm --filter=./example build:h5",
    "build:mp-weixin": "pnpm build && pnpm --filter=./example build:mp-weixin",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc -p . --noEmit",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@babel/generator": "^7.28.3",
    "@babel/types": "^7.28.2",
    "@vue/compiler-sfc": "3.4.21",
    "anymatch": "^3.1.3",
    "ast-kit": "^0.12.2",
    "c12": "2.0.4",
    "chokidar": "^3.6.0",
    "comment-json": "^4.4.1",
    "debug": "^4.4.1",
    "esbuild": "^0.27.1",
    "fast-glob": "^3.3.3",
    "magic-string": "^0.30.18",
    "mlly": "^1.8.0",
    "pathe": "^2.0.3",
    "proper-lockfile": "^4.1.2"
  },
  "devDependencies": {
    "@babel/generator": "^7.23.6",
    "@dcloudio/types": "^3.4.19",
    "@skiyee/eslint-config": "^0.1.0",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.19.11",
    "@types/proper-lockfile": "^4.1.4",
    "@vitest/coverage-v8": "^1.6.1",
    "bumpp": "^10.3.1",
    "eslint": "^9.34.0",
    "lint-staged": "^16.2.6",
    "simple-git-hooks": "^2.13.1",
    "tsdown": "^0.16.6",
    "typescript": "^5.9.2",
    "vite": "^5.4.19",
    "vitest": "^1.6.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,vue}": [
      "pnpm eslint --fix"
    ]
  }
}
